<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/artboard"
    android:layout_width="394.0dp"
    android:layout_height="782.5dp"
    android:background="@drawable/artboard">


    <ImageView
        android:id="@+id/draw2"
        android:layout_width="394.0dp"
        android:layout_height="720.0dp"
        android:src="@drawable/draw56"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/draw1"
            android:layout_width="394.0dp"
            android:layout_height="274.0dp"
            android:src="@drawable/draw57"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="508.5dp"/>
        <RelativeLayout
            android:id="@+id/draw3"
            android:layout_width="380.0dp"
            android:layout_height="60dp"
            android:layout_marginTop="50dp"
            android:padding="10dp"
            android:background="@drawable/draw50"
            android:layout_centerHorizontal="true"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent">
            <Button
                android:id="@+id/mini_search_btn"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:background="@drawable/draw50"
                android:text="검색"
                android:textColor="@color/color_ff000000"
                android:textSize="30dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="290dp"/>
        </RelativeLayout>
</RelativeLayout>