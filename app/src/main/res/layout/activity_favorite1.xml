<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/artboard"
    android:layout_width="394.0dp"
    android:layout_height="1366.0dp"
    android:background="@drawable/artboard">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fadeScrollbars="true"
        android:fillViewport="true">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/draw51"
                android:layout_width="394.0dp"
                android:layout_height="515.0dp"
                android:background="@drawable/draw51"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <Button
                    android:id="@+id/search_btn1"
                    android:text="임영웅 소개"
                    android:textColor="#000000"
                    android:textSize="30dp"
                    android:layout_centerHorizontal="true"
                    android:layout_width="380.0dp"
                    android:layout_height="60dp"
                    android:layout_marginTop="50dp"
                    android:padding="10dp"
                    android:background="@drawable/draw50"
                    android:drawableLeft="@drawable/search"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">
                </Button>


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/draw43"
                android:layout_width="394.0dp"
                android:layout_height="485.0dp"
                android:layout_marginTop="124.5dp"
                android:background="@drawable/draw43"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/text29"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="7.5dp"
                    android:gravity="start|top"
                    android:text="@string/text29"
                    android:textColor="@color/color_ff000000"
                    android:textSize="30sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/draw41"
                    android:layout_width="57.4dp"
                    android:layout_height="181.7dp"
                    android:layout_marginStart="327dp"
                    android:layout_marginTop="160.9dp"
                    android:contentDescription="@string/text29"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/draw42"
                    android:layout_width="255.5dp"
                    android:layout_height="344.5dp"
                    android:layout_marginStart="40dp"
                    android:layout_marginTop="100.5dp"
                    android:contentDescription="@string/text29"
                    android:src="@drawable/draw42"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageButton
                    android:id="@+id/favorite_btn1"
                    android:layout_width="250.5dp"
                    android:layout_height="344.5dp"
                    android:layout_marginStart="28dp"
                    android:layout_marginTop="87.5dp"
                    android:contentDescription="@string/text29"
                    android:src="@drawable/invalid_name"
                    android:scaleType="fitCenter"
                    android:padding="10dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/draw47"
                android:layout_width="394.0dp"
                android:layout_height="1366dp"
                android:layout_marginTop="600dp"
                android:background="@drawable/draw47"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/text30"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="25dp"
                    android:gravity="start|top"
                    android:text="@string/text30"
                    android:textColor="@color/color_ff000000"
                    android:textSize="30sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <RelativeLayout
                    android:id="@+id/draw46"
                    android:layout_width="394.0dp"
                    android:layout_height="126.0dp"
                    android:layout_marginTop="90.5dp"
                    android:background="@drawable/draw46"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/text31"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="119.5dp"
                        android:layout_marginTop="50dp"
                        android:gravity="start|top"
                        android:text="@string/text31"
                        android:textColor="@color/color_ff000000"
                        android:textSize="30sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/text32"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="114dp"
                        android:layout_marginTop="4dp"
                        android:gravity="start|top"
                        android:text="@string/text32"
                        android:textColor="@color/color_ff000000"
                        android:textSize="32.5sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ImageView
                        android:id="@+id/draw45"
                        android:layout_width="283.0dp"
                        android:layout_height="10.0dp"
                        android:layout_marginStart="111dp"
                        android:layout_marginTop="105.6dp"
                        android:src="@drawable/draw45"
                        android:background="@color/color_ffffffff"
                        android:contentDescription="@string/text30"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <RelativeLayout
                        android:id="@+id/image12"
                        android:layout_width="111.0dp"
                        android:layout_height="111.5dp"
                        android:layout_marginTop="4dp"
                        android:background="@drawable/image1"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <RelativeLayout
                            android:id="@+id/draw44"
                            android:layout_width="75.0dp"
                            android:layout_height="75.0dp"
                            android:layout_marginStart="18dp"
                            android:layout_marginTop="18.5dp"
                            android:background="@drawable/draw44"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <TextView
                                android:id="@+id/text33"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="23dp"
                                android:layout_marginTop="0.5dp"
                                android:gravity="start|top"
                                android:text="@string/text33"
                                android:textColor="@color/color_ffffffff"
                                android:textSize="50sp"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />
                        </RelativeLayout>
                    </RelativeLayout>
                </RelativeLayout>
            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>


    </RelativeLayout>
