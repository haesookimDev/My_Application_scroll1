<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/first_main"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/artboard">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fadeScrollbars="true"
        android:fillViewport="true">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <RelativeLayout
                android:id="@+id/page_lay_w"
                android:layout_width="394.0dp"
                android:layout_height="332.5dp"
                android:background="@drawable/page_lay_w"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginTop="450dp">

                <Button
                    android:id="@+id/strem_song"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="27.5dp"
                    android:gravity="center_horizontal|top"
                    android:text="@string/strem_song"
                    android:fontFamily="@font/bmeuljirottf"
                    android:textColor="@color/color_ff000000"
                    android:textSize="25sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:background="@drawable/button_no_color_layer"/>


            </RelativeLayout>



            <RelativeLayout
                android:id="@+id/strem_page_up"
                android:layout_width="394.0dp"
                android:layout_height="450.0dp"
                android:background="@drawable/strem_page_up"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent">
                <RelativeLayout
                    android:id="@+id/mini_home_search_btn_main"
                    android:layout_width="394.0dp"
                    android:layout_height="115.0dp"
                    android:layout_marginTop="16.5dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <Button
                        android:id="@+id/mini_playlist_btn"
                        android:layout_width="200.0dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="210dp"
                        android:layout_marginTop="33.5dp"
                        android:background="@drawable/mini_search_button"
                        android:drawableStart="@drawable/mini_playlistbutton"
                        android:paddingStart="20dp"
                        android:paddingEnd="0dp"
                        android:text="@string/strem_list"
                        android:fontFamily="@font/bmeuljirottf"
                        android:textColor="#000000"
                        android:textSize="30sp" />

                    <Button
                        android:id="@+id/mini_home_btn"
                        android:layout_width="170dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="33.5dp"
                        android:background="@drawable/mini_home_button"
                        android:paddingStart="5dp"
                        android:text="@string/mini_home_text"
                        android:fontFamily="@font/bmeuljirottf"
                        android:textColor="#000000"
                        android:textSize="35sp" />

                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/strem_mid_lay"
                    android:layout_width="394.5dp"
                    android:layout_height="106.0dp"
                    android:background="@drawable/strem_mid_lay"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="344dp">


                    <SeekBar
                        android:id="@+id/seekBar1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_marginStart="15dp"
                        android:layout_marginTop="40dp"
                        android:thumb="@drawable/seekbar_seekbar1_thumb"
                        android:progressDrawable="@drawable/seekbar_seekbar1"/>

                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/strem_album"
                android:layout_width="229.5dp"
                android:layout_height="206.0dp"
                android:background="@drawable/strem_album"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="71dp"
                android:layout_marginTop="184dp">

                <ImageView
                    android:id="@+id/strem_album_um_shadow"
                    android:layout_width="150.0dp"
                    android:layout_height="150.0dp"
                    android:src="@drawable/stream_album_shadow"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="50dp"
                    android:layout_marginTop="10dp"/>

                <ImageView
                    android:id="@+id/strem_album_um"
                    android:layout_width="150.0dp"
                    android:layout_height="150.0dp"
                    android:src="@drawable/boy1_kickit"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="40dp"
                    android:layout_marginTop="10dp"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/strem_buttons"
                android:layout_width="250.5dp"
                android:layout_height="73.5dp"
                android:background="@drawable/strem_buttons"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="70.5dp"
                android:layout_marginTop="300.5dp">

                <ImageView
                    android:id="@+id/strem_black_buttons"
                    android:layout_width="214.0dp"
                    android:layout_height="54.5dp"
                    android:src="@drawable/strem_black_buttons"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="9.5dp"/>
                <RelativeLayout
                    android:id="@+id/strem_s_buttons"
                    android:layout_width="214.0dp"
                    android:layout_height="54.5dp"
                    android:background="@drawable/strem_s_buttons"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="4.5dp">

                    <RelativeLayout
                        android:id="@+id/stream_ss_buttons"
                        android:layout_width="187.5dp"
                        android:layout_height="44.0dp"
                        android:background="@drawable/strem_ss_buttons"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginStart="14dp"
                        android:layout_marginTop="5.5dp">

                        <Button
                            android:id="@+id/stream_play_stop1"
                            android:layout_width="59dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="66dp"
                            android:background="@drawable/button_no_color_layer"
                            android:gravity="start|top"
                            android:text="@string/stream_play"
                            android:fontFamily="@font/bmeuljirottf"
                            android:textColor="@color/color_fff63e4b"
                            android:textSize="30sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            android:onClick="button"/>

                        <Button
                            android:id="@+id/strem_before"
                            android:layout_width="45dp"
                            android:layout_height="wrap_content"
                            android:text="@string/strem_before"
                            android:fontFamily="@font/bmeuljirottf"
                            android:background="@drawable/button_no_color_layer"
                            android:gravity="start|top"
                            android:textColor="@color/color_ff000000"
                            android:textSize="20sp"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="7dp"
                            android:layout_marginTop="6dp"/>

                        <Button
                            android:id="@+id/strem_nex"
                            android:layout_width="45dp"
                            android:layout_height="wrap_content"
                            android:text="@string/strem_nex"
                            android:fontFamily="@font/bmeuljirottf"
                            android:background="@drawable/button_no_color_layer"
                            android:gravity="start|top"
                            android:textColor="@color/color_ff090909"
                            android:textSize="20sp"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="141.5dp"
                            android:layout_marginTop="6dp"/>


                    </RelativeLayout>
                </RelativeLayout>
            </RelativeLayout>



            <RelativeLayout
                android:id="@+id/draw18"
                android:layout_width="394dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="0dp"
                android:layout_marginTop="110dp"
                android:background="@drawable/strem_title"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/text8"
                    android:layout_width="394dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="49.5dp"
                    android:fontFamily="@font/bmeuljirottf"
                    android:gravity="center"
                    android:text="@string/text8"
                    android:textColor="@color/color_ff000000"
                    android:textSize="21.5sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/text9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/bmeuljirottf"
                    android:text="@string/text9"
                    android:gravity="center_horizontal"
                    android:textAlignment="center"
                    android:textColor="@color/color_ff000000"
                    android:textSize="27sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>

