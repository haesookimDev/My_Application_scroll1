<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/artboard"
    android:layout_width="394.0dp"
    android:layout_height="782.0dp"
    android:background="@drawable/tuto_playlist_delete">
    <RelativeLayout
        android:id="@+id/draw134"
        android:layout_width="394.0dp"
        android:layout_height="190.0dp"
        android:background="@drawable/tuto_exp_pure_tool"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent">

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/draw125"
        android:layout_width="393.0dp"
        android:layout_height="363.5dp"
        android:background="@drawable/tuto_exp_tool"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="1dp"
        android:layout_marginTop="418.5dp">
        <TextView
            android:id="@+id/text78"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tuto_op4_6"
            android:gravity="center_horizontal|top"
            android:textColor="@color/color_ff000000"
            android:textSize="22.5sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="130dp"
            android:layout_marginTop="22dp"/>
        <Button
            android:id="@+id/button_go_tuto"
            android:layout_width="328.5dp"
            android:layout_height="53.0dp"
            android:background="@drawable/tuto_ok_btn"
            android:text="@string/tuto_go_tutomain"
            android:textSize="25sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp"
            android:layout_marginTop="150dp">
        </Button>
        <ImageView
            android:id="@+id/image42"
            android:layout_width="100.0dp"
            android:layout_height="100.0dp"
            android:src="@drawable/tuto_finger_success"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="12.5dp"
            android:layout_marginTop="10dp"/>
    </RelativeLayout>
</RelativeLayout>